<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.diycircuits.inputmethod.latin"
>
    <Key
        latin:keyLabel="a"
        latin:keyHintLabel="\@"
        latin:additionalMoreKeys="\@" />
    <Key
        latin:keyLabel="s"
        latin:keyHintLabel="#"
        latin:additionalMoreKeys="#" />
    <Key
        latin:keyLabel="d"
        latin:keyHintLabel="$"
        latin:additionalMoreKeys="$" />
    <Key
        latin:keyLabel="f" 
        latin:keyHintLabel="%"
        latin:additionalMoreKeys="%" />
    <Key
        latin:keyLabel="g"
        latin:keyHintLabel="&amp;"
        latin:additionalMoreKeys="&amp;" />
    <Key
        latin:keyLabel="h"
        latin:keyHintLabel="*"
        latin:additionalMoreKeys="*" />
    <Key
        latin:keyLabel="j"
        latin:keyHintLabel="-"
        latin:additionalMoreKeys="-" />
    <Key
        latin:keyLabel="k"
        latin:keyHintLabel="+"
        latin:additionalMoreKeys="+" />
    <Key
        latin:keyLabel="l"
        latin:keyHintLabel="!"
        latin:additionalMoreKeys="!" />
</merge>
